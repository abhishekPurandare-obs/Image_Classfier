    image: mlflow-image:latest
    # volumes: ["`pwd`/models/:/mlflow/projects/code/models/"]
    environment: [["MLFLOW_TRACKING_URI", "http://ec2-13-232-139-2.ap-south-1.compute.amazonaws.com:5000/"]]

